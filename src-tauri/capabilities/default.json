{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "platforms": ["iOS"],
  "windows": ["main"],
  "webviews": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close"
  ]
}
